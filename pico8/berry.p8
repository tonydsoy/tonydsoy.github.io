pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
px=64 py=64
bx=32 by=32

pmove=3
bmove=1

scene=0

score=0

noquit=1

moveberry=0

function within(px, py, bx, by, a)
	if (px+a)>=bx and (px-a)<=bx and (py+a)>=by and (py-a)<=by then
		return 1
	end
end

function _update()
	if scene==1 then
		if (btn(4)) then scene=1 px=64 py=64 bx=32 by=32 end
		if (btn(0)and px>=1) then px=px-pmove end
		if (btn(1)and px<=118) then px=px+pmove end
		if (btn(2)and py>=1) then py=py-pmove end
		if (btn(3)and py<=118) then py=py+pmove end
		if (within(px, py, bx, by, 5)) then
		 score=score+1
		 bx=rnd(120)
		 by=rnd(120)
		 sfx(01)
	 end
	 if moveberry==1 then
			if (px >= bx) then bx=bx+bmove end
			if (px <= bx) then bx=bx-bmove end
			if (py >= by) then by=by+bmove end
			if (py <= by) then by=by-bmove end
		end
	end
	if scene==0 then
		if (btn(4)) then sfx(0) scene = 1 end
		if (noquit==0) then
			if (btn(5)) then sfx(0) cls() stop() end
		end
	end
end

function _draw()
	if scene==1 then
		cls(3)
		spr(001,px,py)
		spr(003,bx,by)
		print(score,5,5)
	end
	if scene==0 then
		cls(3)
		print("berry game", 45, 64)
		print("press z (🅾️) to start", 25, 75)
		if noquit==0 then
			print("press x (❎) to quit", 25, 85)
		end
	end
end
